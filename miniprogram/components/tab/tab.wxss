image{
  display: block;
}
.tabBox{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 96rpx;
  background:#fff;
  display: flex;
  justify-content: space-around;
  align-items: center;
  box-shadow:0px -4rpx 8rpx 0rpx rgba(0,0,0,0.11);
  z-index: 99;
}
.tabList{
  text-align: center;
  font-size: 24rpx;
  flex-grow: 1;
  color: var(--color);
}
.tabImg{
  width: 48rpx;
  height: 48rpx;
  margin: 0 auto;
}
.tabListColor{
  color:rgba(191,191,191,1);
}

@keyframes show {
  from {
    transform: scale(1.3);
  }
  20% {
    transform: scale(0.8);
  }

  40% {
    transform: scale(1);
  }

  60% {
    transform: scale(0.7);
  }

  80%,100% {
    transform: scale(1);
  }
}

.show {
  animation: show 1s;
}
